
[loggers]
keys=root,LOGGING
 
[handlers]
keys=fileHandler, consoleHandler
 
[formatters]
keys=myFormatter
 
[logger_root]
level=CRITICAL
handlers=consoleHandler
 
[logger_LOGGING]
level=INFO
handlers=fileHandler
qualname=LOGGING
 
[handler_consoleHandler]
class=StreamHandler
level=DEBUG
formatter=myFormatter
args=(sys.stdout,)
 
[handler_fileHandler]
class=FileHandler
formatter=myFormatter
args=("logs/external_file.log",)
 
[formatter_myFormatter]
format=%(asctime)s - %(name)s - %(message)s
datefmt=